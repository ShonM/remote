    "autoload": {
        "psr-0": {
            "Illuminate\\Remote": ""
        }
    },
    "target-dir": "Illuminate/Remote",
    "extra": {
        "branch-alias": {
            "dev-fourtwo": "4.2-dev",
            "dev-master": "4.2-dev"
        }
    },
    "minimum-stability": "dev"
